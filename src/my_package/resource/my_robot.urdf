<?xml version="1.0" ?>
<robot name="AUREA" xmlns:xacro="http://ros.org/wiki/xacro">

  <!-- Link base fictício -->
  <link name="base_link">
    <inertial>
      <mass value="1.0"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.1" iyy="0.1" izz="0.1" ixy="0.0" ixz="0.0" iyz="0.0"/>
    </inertial>
  </link>

  <!-- Integração com Webots -->
  <webots>
    <plugin type="my_package.my_robot_driver.MyRobotDriver"/>
    <device>
      <name>CAM</name>
      <type>Camera</type>
    </device>
  </webots>

</robot>

